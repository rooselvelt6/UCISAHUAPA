<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
  <record id="vista_form_admision_rna" model="ir.ui.view">
  		<field name="name">Formulario RNA</field>
  		<field name="model">admision.admision</field>
  		<field name="inherit_id" ref="admision.admision_form_view"></field>
  		<field name="arch" type="xml">
  			<field name="estadia_hospitalaria" position="before">
  				<field name="estadia" placeholder="Predicción de la estadía con RNA"></field>
  			</field>

        <field name="estadia" position="before">
          <field name="vector_entrada" placeholder="Percepciones del entorno"></field>
        </field>
  		</field>
  	</record> 

  	<record id="vista_tree_admision_rna" model="ir.ui.view">
  		<field name="name">RNA Tree</field>
  		<field name="model">admision.admision</field>
  		<field name="inherit_id" ref="admision.admision_tree_view"></field>
  		<field name="arch" type="xml">
  			<field name="fecha_ingreso_uci" position="after">
  				<field name="estadia"></field>
  			</field>
  		</field>
  	</record>

  </data>
</odoo>